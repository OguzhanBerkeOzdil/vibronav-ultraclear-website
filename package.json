{
  "name": "vibronav-ultraclear",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "echo \"No tests specified. This is a static site.\" && exit 0",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "optimize-images": "node scripts/optimize-images.mjs",
    "blog:build": "node scripts/blog-build.mjs",
    "bib:parse": "node scripts/parse-bib.mjs",
    "sitemap": "node scripts/generate-sitemap.mjs",
    "prebuild": "node scripts/prebuild.mjs",
    "audit:assets": "node scripts/find-unused-assets.mjs",
    "audit:components": "node scripts/find-dead-components.mjs",
    "audit:comprehensive": "node scripts/comprehensive-audit.mjs",
    "normalize:headers": "node scripts/normalize-headers.mjs",
    "clean:assets": "node scripts/find-unused-assets.mjs --delete",
    "type-check": "tsc --noEmit",
    "prepare": "npm run blog:build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/rollup": "^3.1.0",
    "@types/react-router-dom": "^5.3.3",
    "bibtex-parse": "^2.1.0",
    "clsx": "^2.1.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.1.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "sharp": "^0.34.3",
    "swiper": "^11.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.0.6",
    "vite-plugin-svgr": "^4.3.0"
  }
}
